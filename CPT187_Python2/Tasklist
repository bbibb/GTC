from dataclasses import dataclass

@dataclass
class Task:
    name:str = ""
    __state:str = ""

    @property
    def state(self):
        return self.__state

    def setState(self, state):
        self__state = state

class Tasklist
    name:str=""

    def __init__self(self):
        self.__list = []

    def addTask(self, task):
        self.__list.append(task)

    def removeTask(self, task):
        self.__list.pop(task) # pop?
